@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<table class="table table-striped table-hover">
    <thead class="info">
        <tr>
            <td class="display-label">
                @Html.DisplayNameFor(model => model.name)
            </td>

            <td class="display-label">
                @Html.DisplayNameFor(model => model.description)
            </td>

        </tr>
    </thead>

    <tbody>
