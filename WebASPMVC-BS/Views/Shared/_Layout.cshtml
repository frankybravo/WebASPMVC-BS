<!DOCTYPE html>
<html lang="en">
<head >
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Bootstrap</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<side>


</side>
<body>

